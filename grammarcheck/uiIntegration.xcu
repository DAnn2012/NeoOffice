<?xml version='1.0' encoding='UTF-8'?>

<oor:component-data xmlns:oor="http://openoffice.org/2001/registry" xmlns:xs="http://www.w3.org/2001/XMLSchema" oor:name="Addons" oor:package="org.openoffice.Office"> 
	<node oor:name="AddonUI">
		<node oor:name="AddonMenu">
			<node oor:name="org.neooffice.grammarchecker.grammarchecker" oor:op="replace">
				<prop oor:name="Title" oor:type="xs:string">
					<value xml:lang="en-US">Check Grammar</value>
					<value xml:lang="es">Revisar gramática</value>
					<value xml:lang="fr">Vérifier la grammaire</value>
					<value xml:lang="nl">Controleer grammatica</value>
				</prop>
				<prop oor:name="Context" oor:type="xs:string">
					<value>com.sun.star.text.TextDocument</value>
				</prop>
				<node oor:name="Submenu">
					<node oor:name="org.neooffice.grammarchecker.checkDocument" oor:op="replace">
						<prop oor:name="URL" oor:type="xs:string">
							<value>macro:///GrammarGUI.XGrammarGUI.CheckDocument</value>
						</prop>
						<prop oor:name="Title" oor:type="xs:string">
							<value xml:lang="en-US">Entire Document</value>
							<value xml:lang="es">A todo el documento</value>
							<value xml:lang="fr">Document entier</value>
							<value xml:lang="nl">Gehele document</value>
						</prop>
						<prop oor:name="ImageIdentifier" oor:type="xs:string">
							<value/>
						</prop>
						<prop oor:name="Target" oor:type="xs:string">
							<value>_self</value>
						</prop>
						<prop oor:name="Context" oor:type="xs:string">
							<value>com.sun.star.text.TextDocument</value>
						</prop>
					</node>
					<node oor:name="org.neooffice.grammarchecker.checkSelection" oor:op="replace">
						<prop oor:name="URL" oor:type="xs:string">
							<value>macro:///GrammarGUI.XGrammarGUI.CheckSelection</value>
						</prop>
						<prop oor:name="Title" oor:type="xs:string">
							<value xml:lang="en-US">Selected Text</value>
							<value xml:lang="es">Al texto seleccionado</value>
							<value xml:lang="fr">Sélection</value>
							<value xml:lang="nl">Geselecteerde tekst</value>
						</prop>
						<prop oor:name="ImageIdentifier" oor:type="xs:string">
							<value/>
						</prop>
						<prop oor:name="Target" oor:type="xs:string">
							<value>_self</value>
						</prop>
						<prop oor:name="Context" oor:type="xs:string">
							<value>com.sun.star.text.TextDocument</value>
						</prop>
					</node>
				</node>
			</node>
		</node>
	</node>
</oor:component-data>
